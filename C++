#include <iostream>

using namespace std;

int main()
{
    float n1, n2, r;
    
    n1 = 10;
    n2 = 32;
    r = n1 + n2;
    cout<< (" '10' + '32= ");
    cout<< r;
    
	r = n1 - n2;
    cout<< ("\n '10' - '32= ");
    cout<< r;

	    r = n1 * n2;
    cout<< ("\n '10' * '32= ");
    cout<< r;
	
    r = n1 / n2;
    cout<< ("\n '10' / '32= ");
    cout<< r;
    system("pause");
}
